/*轮播图*/
#map{
	position: relative;
	height: 600px;
	border-top: 2px solid #CDC9A5;
	/*border: 1px solid #bbb;*/
	background-color:#EEE8CD;
}
#slidorion1 {
	width: 1250px;			/* Set to slidorion width. Is equal to #slider + #accordion width */
	height: 600px;			/* Set to slidorion height. Is equal to #slider and #accordion height */		position: absolute;
	left: 2%;
	float: left;
	}
#slider {
	width: 105%;			/* Set to slider width */
	height: 100%;			/* Set to slider height */
	position: relative;
	float: left;
	overflow: hidden;		/* Hides the animations */
}

.slider-image {
	position: relative;
}

#accordion {
	width: 13%;
	margin-left: 105%;			/* Set to accordion width */
	height: 100%;			/* Set to image height */
	font-family: Verdana;
	background: #eee;
	-webkit-box-shadow: -3px 0px 3px -1px rgba(51,51,51,0.3);
	-moz-box-shadow: -3px 0px 3px -1px rgba(51,51,51,0.3);
	box-shadow: -3px 0px 3px -1px rgba(51,51,51,0.3);
	position: absolute;
	/*right: 0;*/
	/*z-index:900;*/
	/*float: left;*/
}

#accordion .header {
	height: 150px;
*/	border: none;
	border-top: 1px solid #ccc;
	text-align: center;
	background-color: #FDF5E6;
}

#accordion .header a {
	background: none;
	margin-top:48px;
	text-decoration: none;
	font-weight: bold;
	font-size: 50px;
	font-family: fonts1;
	display: block;
	color: black;
}

#accordion .header a:hover {
	cursor: pointer;

}
#map .name .name_1{
	width:5%;
	height: 100%;
	margin-top: 1%;
	/*border: 1px solid black;*/
	margin-left: 83%;
}
#map .name .name_2{
	width: 5%;
	height: 100%;
	margin-top:0%;
	font-family: fonts;
	/*border:1px solid black;*/
	margin-left: 90%;
}
#map .name p{
	font-size: 131px;
	font-weight: 800;
	font-family: fonts;
}
